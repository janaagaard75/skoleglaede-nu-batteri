{
  "name": "skoleglaede-nu-batteri",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "private": true,
  "packageManager": "yarn@4.9.3",
  "scripts": {
    "android": "expo run:android",
    "build": "yarn eas build --profile production",
    "build-ios": "yarn eas build --platform ios --profile production",
    "check-lint": "eslint --format unix --max-warnings 0",
    "check-types": "tsc",
    "check-formatting": "prettier --check ./src/",
    "eas": "yarn dlx --package eas-cli --quiet eas",
    "eas-build-pre-install": "corepack enable",
    "ios": "expo run:ios",
    "optimize-icons": "npx --package imagemin-cli imagemin ./assets-src/*.png --out-dir ./assets",
    "start": "yarn install && expo start",
    "submit-ios": "yarn eas submit --platform ios --profile production",
    "test": "jest --watchAll",
    "update-dependencies": "yarn upgrade-interactive && rm yarn.lock && yarn install"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "expo": {
    "doctor": {
      "reactNativeDirectoryCheck": {
        "listUnknownPackages": false
      }
    }
  },
  "dependencies": {
    "@babel/core": "^7.28.4",
    "@expo/metro-runtime": "^6.1.2",
    "@expo/vector-icons": "^15.0.2",
    "@react-navigation/native": "^7.1.8",
    "@stylistic/eslint-plugin": "^5.4.0",
    "@types/react": "~19.1.13",
    "eslint": "^9.36.0",
    "eslint-formatter-unix": "^8.40.0",
    "eslint-plugin-filename-export": "^2.0.0",
    "eslint-plugin-perfectionist": "^4.15.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "6.0.0-rc1",
    "eslint-plugin-react-hooks-extra": "^1.53.1",
    "eslint-plugin-react-native": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "expo": "^54.0.0",
    "expo-camera": "~17.0.8",
    "expo-constants": "~18.0.9",
    "expo-font": "~14.0.8",
    "expo-linking": "~8.0.8",
    "expo-router": "~6.0.7",
    "expo-secure-store": "~15.0.7",
    "expo-splash-screen": "~31.0.10",
    "expo-status-bar": "~3.0.8",
    "expo-system-ui": "~6.0.7",
    "expo-updates": "~29.0.11",
    "html-entities": "^2.6.0",
    "prettier": "^3.6.2",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.4",
    "react-native-reanimated": "~4.1.0",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "15.12.1",
    "react-native-web": "^0.21.0",
    "react-native-worklets": "^0.5.1",
    "tailwindcss": "^3.4.17",
    "typescript": "~5.9.2",
    "typescript-eslint": "^8.44.0"
  }
}
