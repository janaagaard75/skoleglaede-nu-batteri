{
  "name": "skoleglaede-nu-batteri",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "private": true,
  "packageManager": "yarn@1.22.22",
  "scripts": {
    "android": "expo run:android",
    "build": "eas build --profile production",
    "build-ios": "eas build --platform ios --profile production",
    "check-lint": "eslint . --format unix",
    "check-types": "tsc",
    "ios": "expo run:ios",
    "optimize-icons": "npx --package imagemin-cli imagemin ./assets-src/*.png --out-dir ./assets",
    "start": "yarn install && expo start",
    "submit": "eas submit --platform ios --profile production",
    "test": "jest --watchAll",
    "update-dependencies": "npx expo install --check"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "expo": {
    "doctor": {
      "reactNativeDirectoryCheck": {
        "listUnknownPackages": false
      }
    }
  },
  "dependencies": {
    "@expo/vector-icons": "^14.0.4",
    "@react-navigation/native": "^7.0.0",
    "expo": "~52.0.17",
    "expo-camera": "~16.0.9",
    "expo-constants": "~17.0.3",
    "expo-font": "~13.0.1",
    "expo-linking": "~7.0.3",
    "expo-router": "~4.0.11",
    "expo-secure-store": "~14.0.0",
    "expo-splash-screen": "~0.29.16",
    "expo-status-bar": "~2.0.0",
    "expo-system-ui": "~4.0.5",
    "expo-updates": "~0.26.10",
    "expo-web-browser": "~14.0.1",
    "html-entities": "^2.5.2",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-native": "0.76.3",
    "react-native-reanimated": "~3.16.2",
    "react-native-safe-area-context": "4.12.0",
    "react-native-screens": "~4.1.0",
    "react-native-svg": "15.8.0",
    "react-native-web": "~0.19.13",
    "tailwindcss": "^3.4.15"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@types/react": "~18.3.12",
    "eslint": "^8.57.1",
    "eslint-config-expo": "~8.0.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "prettier": "^3.4.1",
    "typescript": "~5.3.3"
  }
}
