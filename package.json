{
  "name": "skoleglaede-nu-batteri",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "private": true,
  "packageManager": "yarn@4.9.3",
  "scripts": {
    "android": "expo run:android",
    "build": "eas build --profile production",
    "build-ios": "eas build --platform ios --profile production",
    "check-lint": "eslint --format unix --max-warnings 0",
    "check-types": "tsc",
    "eas-build-pre-install": "corepack enable",
    "ios": "expo run:ios",
    "optimize-icons": "npx --package imagemin-cli imagemin ./assets-src/*.png --out-dir ./assets",
    "start": "yarn install && expo start",
    "submit-ios": "eas submit --platform ios --profile production",
    "test": "jest --watchAll",
    "update-dependencies": "npx expo install --fix && rm yarn.lock && yarn install"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "expo": {
    "doctor": {
      "reactNativeDirectoryCheck": {
        "listUnknownPackages": false
      }
    }
  },
  "dependencies": {
    "@babel/core": "^7.26.0",
    "@expo/vector-icons": "^14.1.0",
    "@react-navigation/native": "^7.0.0",
    "@stylistic/eslint-plugin": "^5.2.3",
    "@types/react": "~19.0.10",
    "eslint": "^9.33.0",
    "eslint-formatter-unix": "^8.40.0",
    "eslint-plugin-filename-export": "^2.0.0",
    "eslint-plugin-perfectionist": "^4.15.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "6.0.0-rc.1",
    "eslint-plugin-react-hooks-extra": "^1.52.5",
    "eslint-plugin-react-native": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "expo": "^53.0.20",
    "expo-camera": "~16.1.11",
    "expo-constants": "~17.1.7",
    "expo-font": "~13.3.2",
    "expo-linking": "~7.1.7",
    "expo-router": "~5.1.4",
    "expo-secure-store": "~14.2.3",
    "expo-splash-screen": "~0.30.10",
    "expo-status-bar": "~2.2.3",
    "expo-system-ui": "~5.0.10",
    "expo-updates": "~0.28.17",
    "html-entities": "^2.5.2",
    "prettier": "^3.4.1",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-native": "0.79.5",
    "react-native-reanimated": "~3.17.4",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.11.1",
    "react-native-svg": "15.11.2",
    "react-native-web": "^0.20.0",
    "tailwindcss": "^3.4.15",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.39.1"
  }
}
